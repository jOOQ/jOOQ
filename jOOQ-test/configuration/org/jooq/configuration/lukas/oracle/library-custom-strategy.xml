            <records>false</records>
            <pojos>true</pojos>
            <jpaAnnotations>true</jpaAnnotations>
        </generate>
        <target>
            <packageName>org.jooq.test.oracle3.generatedclasses</packageName>
            <directory>./src</directory>
        </target>
    </generator>
</configuration>
