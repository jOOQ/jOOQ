        <sqlite.version>3.39.4.1</sqlite.version>
        <derby.version>10.14.2.0</derby.version>
        <hsqldb.version>2.7.1</hsqldb.version>

        <!-- JDBC drivers for jOOQ-xyz-extensions modules and vendor-specific API access -->
